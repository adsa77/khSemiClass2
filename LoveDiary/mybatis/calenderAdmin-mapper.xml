<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CalenderAdminMapper">
	<select id="getCalenderAdminCnt" resultType="int">
		SELECT COUNT(NO)
		FROM HOLIDAY
		WHERE DEL_YN = 'N'
	</select>
	
		<!-- 공지 목록 -->
	<select id="selectCalenderAdminList" parameterType="com.kh.love.admin.calender.vo.CalenderAdminPageVo" resultType="com.kh.love.admin.calender.vo.CalenderAdminPageVo">
		SELECT *
		FROM HOLIDAY
		
	</select>
	
	

</mapper>