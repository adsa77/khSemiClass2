<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AdminMapper">
	<select id="loginAdmin" resultType="com.kh.love.admin.vo.AdminVo">
		SELECT *
		FROM ADMIN
		WHERE ID = #{id}
		AND PWD = #{pwd}
	</select>
</mapper>